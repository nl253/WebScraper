  "homepage": "https://github.com/nl253/WebScraper/blob/master/README.md",
  "keywords": [
    "http",
    "async",
    "webscrape",
    "webscraper",
    "spider",
    "scraper",
    "scrape"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nl253/WebScraper"
  },
  "bugs": {
    "url": "https://github.com/nl253/WebScraper/issues"
  },
  "author": "Norbert Logiewa <norbertlogiewa96@gmail.com>",
  "maintainers": [
    {
      "email": "norbertlogiewa96@gmail.com",
      "name": "Norbert Logiewa"
    }
  ],
  "contributors": [
    {
      "email": "norbertlogiewa96@gmail.com",
      "name": "Norber Logiewa",
      "url": "https://github.com/nl253"
    }
  ],
  "engines": {
    "node": ">=10.15.1"
  },
  "engineStrict": true,
  "browserslist": [],
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "node-fetch": "^2.6.0",
    "sequelize": "^5.21.2",
    "sqlite3": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^24.9.0",
    "jsdoc": "3.6.3",
    "jsonlint": "^1.6.3",
    "license-checker": "^25.0.1",
    "markdownlint": "^0.17.2",
    "markdownlint-cli": "^0.19.0",
    "remark": "^11.0.2",
    "remark-cli": "^7.0.1",
    "remark-lint": "^6.0.5",
    "remark-preset-lint-consistent": "^2.0.3",
    "remark-preset-lint-markdown-style-guide": "^2.1.3",
    "remark-preset-lint-recommended": "^3.0.3",
    "retire": "^2.0.3",
    "write-good": "^1.0.2"
  },
  "main": "./index.js",
  "directories": {
    "example": "./examples",
    "doc": "./docs",
    "lib": "./"
  },
  "files": [
    "*.js",
    "exporting/*.js"
  ],
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended",
      "remark-preset-lint-markdown-style-guide",
      "remark-preset-lint-consistent"
    ]
  },
  "private": false,
  "scripts": {
    "example": "node examples/jobsite.js",
    "example-2": "node examples/gh.js",
    "test": "npx jest",
    "doc": "echo -e 'GENERATING DOCS\\n'; rm -rf ./docs; npx jsdoc -d ./docs -R ./README.md ./*.js ./exporting/*.js",
    "doc:publish": "echo -e 'PUBLISHING DOCS\\n'; npm run doc; git add ./docs; git commit -am 'Update docs' -n; npm version patch; git push",
    "doc:open": "npm run doc; $BROWSER ./docs/index.html",
    "loc": "tokei -- $(git ls-files | grep -E '\\.js$' | grep -v docs | grep -v bin)",
    "lint": "npx eslint *.js",
    "prepublishOnly": "npm run doc:publish"
  }
}
